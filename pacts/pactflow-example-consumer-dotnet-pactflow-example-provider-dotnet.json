{
  "consumer": {
    "name": "pactflow-example-consumer-dotnet"
  },
  "provider": {
    "name": "pactflow-example-provider-dotnet"
  },
  "interactions": [
    {
      "description": "A valid GET request for Date Validation",
      "providerState": "There is data",
      "request": {
        "method": "get",
        "path": "/products"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": "27",
            "name": "burger",
            "type": "food"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}